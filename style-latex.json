{
    "page": {
        "scale": {
            "position": {
                "x": 2,
                "y": 2
            },
            "size": {          
                "h": 2,
                "w": 2
            }
        },
        "margin": {
            "h": 1,
            "w": 1
        }
    },
    "style": {
        "base": {
            "node": {
                "object": {
                    "tikz": {
                        "shape": "rectangle",
                        "fill": "white",
                        "draw": "black",
                        "minimum width": "1cm",
                        "minimum height": "0.5cm",
                        "rounded corners": "0.005cm",
                        "line width": "0.02cm",
                        "text opacity": "1",
                        "font": "\\footnotesize",
                        "inner sep": "0pt"
                    }
                },
                "text": {
                    "latex": {
                        "flags": {
                            "font": "\\sffamily",
                            "size": "\\small"
                        }
                    }
                }
            },
            "edge": {
                "object": {
                    "tikz": {
                        "draw": "black",
                        "line width": "0.02cm",
                        "rounded corners": "0.05cm"
                    }
                },
                "arrow": {
                    "tikz": {
                        "default": {
                            "shape": "Circle",
                            "width": "0.2cm",
                            "length": "0.2cm"
                        },
                        "start": {
                            "shape": "Circle",
                            "width": "0.2cm",
                            "length": "0.2cm"
                        },
                        "end": {
                            "shape": "Triangle",
                            "width": "0.2cm",
                            "length": "0.2cm"
                        }
                    }
                },
                "text": {
                    "tikz": {
                        "default": {
                            "pos": "0.5"
                        },
                        "start": {
                            "pos": "0.1"
                        },
                        "end": {
                            "pos": "0.9"
                        }
                    },
                    "latex": {
                        "default": {
                            "flags": {
                                "font": "\\sffamily",
                                "size": "\\small"
                            }
                        },
                        "start": {
                            "flags": {
                                "font": "\\sffamily",
                                "size": "\\small"
                            }
                        },
                        "end": {
                            "flags": {
                                "font": "\\sffamily",
                                "size": "\\small"
                            }
                        }
                    }
                }
            }
        },
        "group": {
            "node": {
                "object": {
                    "tikz": {
                        "draw": "gray"
                    }
                }
            }
        },

        "label": {
            "node": {
                "object": {
                    "tikz": {
                        "draw": "white!0",
                        "line width": "0.00cm",
                        "text opacity": "1"
                    }
                },
                "text": {
                    "latex": {
                        "flags": {
                            "font": "\\sffamily",
                            "size": "\\normalsize"
                        }
                    }
                }
            }
        },
        "dashed": {
            "edge": {
                "object": {
                    "tikz": {
                        "dashed": true
                    }
                }
            }
        },
        "soft": {
            "edge": {
                "object": {
                    "tikz": {
                        "draw": "red",
                        "line width": "0.01cm"
                    }
                }
            }
        },
        "bend_right": {
            "edge": {
                "object": {
                    "tikz": {
                        "bend": "right"
                    }
                }
            }
        },
        "thick": {
            "edge": {
                "object": {
                    "tikz": {
                        "line width": "0.04cm"
                    }
                }
            }
        }
    }
}